n=int(input())
k = 0
n1 = n - n//2
for i in range(1, n1 + 1):
    for j in range(1, n+1):
        
        print(k * n + j, end = '')
        print(' ',end="")
    print()
    k = k + 2
n2 = n - n1
if n % 2 == 0:
    k = n - 1
else:
    k = n - 2
for i in range(1, n2 + 1):
    for j in range(1, n + 1):
        
        print(k * n + j, end = '')
        print(' ',end="")
    print()
    k = k - 2
